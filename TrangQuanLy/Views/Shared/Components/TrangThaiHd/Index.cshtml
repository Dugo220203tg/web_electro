@model IEnumerable<TrangQuanLy.Models.TrangThaiHoaDonVM>

@{
    var selectedTrangThai = ViewBag.SelectedTrangThai as int?;
}

@foreach (var item in Model)
{
    if (item.MaTrangThai == selectedTrangThai)
    {
        <option value="@item.MaTrangThai" selected="selected">@item.TenTrangThai</option>
    }
    else
    {
        <option value="@item.MaTrangThai">@item.TenTrangThai</option>
    }
}

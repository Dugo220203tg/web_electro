@model IEnumerable<TDProjectMVC.ViewModels.DanhGiaVM>

@foreach (var item in Model)
{
    <li>
        <div class="review-heading">
            <h5 class="name">@item.MaKH</h5>
            <p class="date">@item.Ngay</p>
            <div class="review-rating">
                @for (int i = 1; i <= 5; i++)
                {
                    if (i <= item.Sao)
                    {
                        <i class="fa fa-star"></i>
                    }
                    else
                    {
                        <i class="fa fa-star-o empty"></i>
                    }
                }
            </div>
        </div>
        <div class="review-body">
            <p>@item.NoiDung</p>
        </div>
    </li>
}